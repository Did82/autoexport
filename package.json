{
  "name": "autoexport",
  "type": "module",
  "workspaces": ["packages/*"],
  "scripts": {
    "dev": "bun --filter '*' dev",
    "build": "bun --filter '*' build",
    "test": "bun --filter '*' test",
    "lint": "bun --filter '*' lint",
    "format": "bun --filter '*' format",
    "start": "bun --filter '*' start",
    "prisma:generate": "bun --filter export-back prisma:generate"
  }
}
